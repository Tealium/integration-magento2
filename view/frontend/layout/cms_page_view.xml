<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="after.body.start">
            <referenceBlock name="tealium_generic" remove="true"/>
            <block class="Tealium\Tags\Block\Template" name="tealium_product" as="tealium_product" template="tealium_tags/cms.phtml" before="tealium_base"  >
                <action method="setType">
                    <argument name="tealium_type" xsi:type="string">cms page</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>
